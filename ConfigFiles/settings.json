{
    "breadcrumbs.enabled": true,
    "editor.cursorBlinking": "smooth",
    "editor.fontFamily": "\"Fira Code Retina\", Hack, Consolas",
    "editor.fontSize": 13,
    "editor.fontLigatures": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "window.zoomLevel": 1,
    "workbench.colorCustomizations": {},
    "workbench.colorTheme": "Winter is Coming (Dark Blue - No Italics)",
    "todohighlight.exclude": "{**/node_modules/**,**/bower_components/**,**/dist/**,**/build/**,**/.vscode/**,**/_output/**,**/*.min.*,**/*.map}",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "files.insertFinalNewline": false,
    "[csharp]": {
        "editor.defaultFormatter": "ms-vscode.csharp"
    },
    "[html]": {
        "editor.defaultFormatter": "lonefy.vscode-JS-CSS-HTML-formatter"
    },
    "workbench.iconTheme": "material-icon-theme",
    "terminal.integrated.shell.windows": "C:\\Program Files\\PowerShell\\6\\pwsh.exe",
    "terminal.integrated.env.windows": {
        "LC_ALL": "C.UTF-8"
    }
}
